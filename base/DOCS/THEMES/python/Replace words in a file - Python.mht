def replace_words(text, word_dic):
    &quot;&quot;&quot;
    take a text and replace words that match a key <strong class=3D"high=
light">in</strong> a dictionary with
    the associated value, return the changed text
    &quot;&quot;&quot;
    rc =3D re.compile('|'.join(map(re.escape, word_dic)))
    def translate(match):
        return word_dic[match.group(0)]
    return rc.sub(translate, text)
