.futu_alert_outer {
	position: fixed;
	top: 0; left: 0;
	width: 100%;
	
	z-index:9999;
}
.futu_alert_outer .frame {
	position:relative;
	z-index:9999;
}
.futu_alerts_holder {
	position:absolute;
	left:69%; top:0;
	width:30%;
	z-index:9999;
}

.futu_alerts_holder .futu_alert {
	position:absolute;
	top:-1000px;
	background-color:#FFEBA0;
	font:1em Tahoma, sans-serif;
	border:1px solid #FFFFFF;
	margin-left:60px;
